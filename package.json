{
	"name": "ext-import",
	"displayName": "Ext-import",
	"description": "provides import completion for files with any extension",
	"version": "1.0.1",
	"publisher": "sodiac",
	"icon": "icon.png",
	"repository": {
		"type": "git",
		"url": "https://github.com/sodiac/ext-import.git"
	},
	"engines": {
		"vscode": "^1.38.0"
	},
	"keywords": [
		"import",
		"extension",
		"intellisense",
		"autocomplete",
		"path"
	],
	"categories": [
		"Other"
	],
	"contributes": {
		"configuration": {
			"type": "object",
			"title": "ext-import",
			"properties": {
				"ext-import.import": {
					"type": "array",
					"default": [
						".json",
						".txt",
						".html"
					],
					"description": "wich files are allowed to import",
					"scope": "resource"
				}
			}
		}
	},
	"activationEvents": [
		"*"
	],
	"main": "./out/extension.js",
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.14.17",
		"@types/vscode": "^1.38.0",
		"glob": "^7.1.4",
		"mocha": "^6.1.4",
		"tslint": "^5.12.1",
		"typescript": "^3.3.1",
		"vscode-test": "^1.0.2"
	}
}
